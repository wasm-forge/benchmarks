type Error = variant {
    InvalidCanister;
    CanisterError: text;
};

type Result = variant {
  Ok: text;
  Err: Error;
};


type QueryResult = variant {
  Ok: vec vec opt text;
  Err: Error;
};

service : {
    "query": (text) -> (QueryResult);
    create_tables: () -> ();
    create_indices: () -> ();
    add_users: (offset: nat64, count: nat64) -> (Result);
    add_orders: (offset: nat64, count: nat64, id_mod: nat64) -> (Result);
}
